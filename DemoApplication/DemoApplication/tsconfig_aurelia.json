{
  "compilerOptions": {
    "target": "es5",
    "lib": [ "es5", "dom", "es2015" ],
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": true
  },
  "include": [
    "_app/**/*"
  ],
  "exclude": [
    "jspm_packages",
    "node_modules"
  ]
}